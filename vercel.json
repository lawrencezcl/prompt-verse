{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sin1"],
  "crons": [
    {
      "path": "/api/cron/collect-prompts",
      "schedule": "0 */4 * * *"
    },
    {
      "path": "/api/cron/update-trends",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/cleanup-analytics",
      "schedule": "0 0 * * *"
    }
  ]
}